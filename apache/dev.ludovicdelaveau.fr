<VirtualHost *:80>
	ServerName dev.ludovicdelaveau.fr
	ServerAdmin webmaster@ludovicdelaveau.fr

	AliasMatch ^/ere/admin_media(/[[\w]+/]*[^/]*\.css) /usr/share/pyshared/django/contrib/admin/media/$1
	AliasMatch ^/ere/site_media(/[[\w]+/]*[^/]*\.(css|info)) /opt/repos/django/ere/media/$1
	
	<Directory /opt/repos/django/ere/media/>
		Order deny,allow
		Allow from all
	</Directory>

	WSGIScriptAlias / /opt/repos/django/ere/apache/ere.wsgi

	ErrorLog /var/log/apache2/error.log
	CustomLog /var/log/apache2/access.log combined
	LogLevel warn
</VirtualHost>
